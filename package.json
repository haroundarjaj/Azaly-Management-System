        "directories": {
            "buildResources": "assets"
        },
        "mac": {
            "icon": "build/icon.png",
            "target": "tar.gz"
        },
        "linux": {
            "category": "Utils",
            "icon": "build/icon.png",
            "packageCategory": "utils",
            "maintainer": "DarTec",
            "asar": true,
            "target": [
                "deb",
                "tar.xz"
            ],
            "synopsis": "Azaly merch management system"
        },
        "win": {
            "icon": "build/icon.png",
            "target": "nsis"
        }
