    "scripts": {
        "start": "react-app-rewired  start",
        "build": "cross-env GENERATE_SOURCEMAP=false react-app-rewired build",
        "test": "react-app-rewired test --env=node",
        "eject": "react-app-rewired eject",
        "build-docs": "babel-node --presets @babel/preset-env src/app/main/documentation/material-ui-components/build.js",
        "analyze": "react-app-rewired build && source-map-explorer 'build/static/js/*.js' --html analyze-result.html",
        "lint": "eslint",
        "react-build": "react-app-rewired build",
        "server-build": "babel server --out-dir build-server",
        "server-start": "nodemon --watch server --exec babel-node server/server.js",
        "electron-dev": "node scripts/dev.js scripts/start.js",
        "build-electron": "electron-builder",
        "electron-pack": "electron-builder -c.extraMetadata.main=\"main.js\"",
        "preelectron-pack": "npm run react-build && npm run server-build",
        "ebuild": "npm run build && npm run build-electron -wml",
        "dev": "concurrently \"npm start\" \"wait-on http://localhost:3333 && electron .\""
    },
    "engines": {
        "node": ">=14.0.0",
        "npm": ">=7.0.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 3 safari version"
        ]
    }
}
